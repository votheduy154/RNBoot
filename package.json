{
  "name": "RNBoot",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "ios:rl": "react-native run-ios --simulator=\"iPhone 6\" --configuration Release",
    "android:rl": "react-native run-android --variant=release",
    "android:signedAPK": "cd android && ./gradlew assembleRelease",
    "cp:ios:rl": "code-push release-react RNBoot_ios ios --deploymentName Production -m --noDuplicateReleaseError",
    "cp:android:rl": "code-push release-react RNBoot_android android --deploymentName Production -m --noDuplicateReleaseError"
  },
  "dependencies": {
    "react": "16.0.0",
    "react-native": "0.51.0",
    "react-native-code-push": "^5.2.1",
    "react-native-device-info": "^0.13.0",
    "react-native-facebook-account-kit": "^0.9.0",
    "react-native-fbsdk": "^0.6.3",
    "react-native-image-picker": "^0.26.7",
    "react-native-linear-gradient": "^2.3.0",
    "react-native-navigation": "^1.1.305",
    "react-native-svg": "^6.0.1-rc.1",
    "react-native-vector-icons": "^4.4.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "babel-jest": "21.2.0",
    "babel-preset-react-native": "4.0.0",
    "eslint": "^4.13.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-react-native": "^3.2.0",
    "jest": "21.2.1",
    "prettier": "^1.9.1",
    "react-test-renderer": "16.0.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
